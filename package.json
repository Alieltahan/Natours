{
  "name": "livtours",
  "version": "1.0.0",
  "description": "Nature tours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/index.css -w",
    "compile:sass": "node-sass sass/main.scss css/index.comp.css -w",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/syle.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [
    "Nature",
    "Tours"
  ],
  "author": "Ali Eltahan",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^5.0.0"
  }
}
